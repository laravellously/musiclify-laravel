  "dockerfileLines": [
    "FROM webdevops/php-nginx:8.2-alpine",
    "WORKDIR /app",
    "COPY . .",
    "RUN composer install --no-interaction --optimize-autoloader --no-dev",
    "RUN php artisan config:cache",
    "RUN php artisan route:cache",
    "RUN php artisan view:cache",
    "RUN chown -R :www-data ."
